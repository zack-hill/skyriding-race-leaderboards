{
  "UploadUrl": "https://race-leaderboards.uk.r.appspot.com/upload",
  "ScanIntervalSeconds": 60,
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning",
        "Microsoft.AspNetCore.Mvc.Internal": "Warning",
        "Microsoft.AspNetCore.Authentication": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "%appdata%/SkyridingRaceLeaderboards/log.txt"
        }
      }
    ]
  }
}
